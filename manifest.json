{
  "id": "com.progressio.idcatalog",
  "name": "ID Catalog Builder",
  "version": "1.0.0",
  "host": [
    {
      "app": "ID",
      "minVersion": "18.0"
    }
  ],
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "main",
      "label": {
        "default": "Catalog Builder"
      },
      "minimumSize": {
        "width": 400,
        "height": 600
      },
      "maximumSize": {
        "width": 1200,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 450,
        "height": 800
      },
      "preferredFloatingSize": {
        "width": 600,
        "height": 900
      }
    }
  ],
  "icons": [
    {
      "width": 24,
      "height": 24,
      "path": "assets/icons/icon-24.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "dark",
        "lightest",
        "light",
        "medium",
        "darkest",
        "all"
      ],
      "species": [
        "generic"
      ]
    }
  ],
  "requiredPermissions": [
    {
      "name": "localFileSystem",
      "access": "readWrite"
    },
    {
      "name": "network",
      "domains": "all"
    },
    {
      "name": "clipboard",
      "access": "readWrite"
    },
    {
      "name": "allowCodeGenerationFromStrings"
    }
  ]
}
