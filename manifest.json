{
  "id": "com.progressio.idcatalog",
  "name": "ID Catalog Builder",
  "version": "1.0.0",
  "main": "src/index.html",
  "host": {
    "app": "ID",
    "minVersion": "18.0"
  },
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "main",
      "main": "src/index.html",
      "label": {
        "default": "Catalog Builder"
      },
      "icons": [],
      "minimumSize": {
        "width": 400,
        "height": 600
      },
      "preferredDockedSize": {
        "width": 450,
        "height": 800
      }
    }
  ],
  "requiredPermissions": {
    "localFileSystem": "fullAccess",
    "network": {
      "domains": "all"
    },
    "launchProcess": {
      "schemes": ["https", "mailto", "file"]
    },
    "clipboard": "readAndWrite"
  }
}
