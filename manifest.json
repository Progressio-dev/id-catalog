{
  "id": "com.progressio.idcatalog",
  "name": "ID Catalog Builder",
  "version": "1.0.0",
  "main": "src/index.html",
  "host": {
    "app": "ID",
    "minVersion": "18.0"
  },
  "manifestVersion": 5,
  "entrypoints": [
    {
      "type": "panel",
      "id": "main",
      "main": "src/index.html",
      "label": {
        "default": "Catalog Builder"
      },
      "minimumSize": {
        "width": 400,
        "height": 600
      },
      "maximumSize": {
        "width": 1200,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 450,
        "height": 800
      },
      "preferredFloatingSize": {
        "width": 600,
        "height": 900
      }
    }
  ],
  "requiredPermissions": [
    {
      "name": "localFileSystem",
      "access": "readWrite"
    },
    {
      "name": "network",
      "domains": "all"
    },
    {
      "name": "clipboard",
      "access": "readWrite"
    }
  ]
}
